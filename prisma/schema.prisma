generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model accountAdmin {
  id        String    @id @default(cuid())
  name      String?
  email     String    @unique
  password  String
  role      AdminRole @default(admin)
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}

enum AdminRole {
  superadmin
  admin
}
